{
  "name": "postbot-renderer",
  "version": "0.1.0",
  "private": true,
  "main": "src/renderer.js",
  "scripts": {
    "start": "node src/server.js",
    "render": "node scripts/render.js --template templates/sample_template.json --mapping examples/mapping.json --out out.png",
    "dev": "node src/server.js",
    "build:plugin": "npm --prefix figma-plugin run build",
    "watch:plugin": "npm --prefix figma-plugin run watch",
    "enqueue:mock": "node scripts/enqueue_from_mock_api.js",
    "smoke:e2e": "node scripts/e2e_smoke_docker.js",
    "render:batch": "node scripts/batch_render_and_test.js",
    "render:admin-static": "node scripts/render_with_admin_static.js",
    "start:all": "bash ./scripts/start_all.sh",
    "start:all:win": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/start_all.ps1",
    "test": "jest --runInBand"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.400.0",
    "@aws-sdk/s3-request-presigner": "^3.927.0",
    "axios": "^1.4.0",
    "bullmq": "^5.63.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "minimist": "^1.2.8",
    "puppeteer-core": "^20.9.0",
    "sharp": "^0.32.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-react": "^7.22.5",
    "babel-jest": "^29.6.0",
    "jest": "^29.0.0"
  }
}
